DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
cd ../../external/msgpack-c
./bootstrap
./configure
cd $DIR
./msgpack_gyp -DTO=ios
cd ../../external/msgpack-c
xcodebuild -project msgpack.xcodeproj -sdk iphonesimulator7.1 -arch i386
mkdir -p build/Default-iphonesimulator
cp -af $DIR/build/Default-iphonesimulator/libmsgpack.a ./build/Default-iphonesimulator/
